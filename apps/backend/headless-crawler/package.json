{
  "name": "headless-crawler",
  "version": "0.1.0",
  "bin": {
    "headless-crawler": "bin/headless-crawler.js"
  },
  "scripts": {
    "bootstrap": "aws-vault exec cdk-bootstrap-user -- cdk bootstrap  --cloudformation-execution-policies arn:aws:iam::aws:policy/AdministratorAccess",
    "deploy": "aws-vault exec cdk-deploy -- cdk deploy",
    "verify:e-t-crawler": "ts-node lib/E-T-crawler/playground/index.ts",
    "verify:job-detail-extractor": "ts-node lib/jobDetail/extractor/playground/index.ts",
    "verify:transform-jobDetail": "ts-node lib/T-jobDetail/playground/index.ts",
    "verify:job-number-crawler": "ts-node lib/scripts/verify-job-number-crawler.ts",
    "type-check": "pnpm exec tsc --noEmit"
  },
  "devDependencies": {
    "@sparticuz/chromium": "^138.0.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/node": "24.10.13",
    "aws-cdk": "2.1030.0",
    "esbuild": "^0.25.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.840.0",
    "@sho/models": "workspace:*",
    "aws-cdk-lib": "2.238.0",
    "constructs": "^10.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.0",
    "effect": "^3.16.5",
    "linkedom": "0.18.12",
    "octokit": "^5.0.3",
    "playwright": "^1.55.1"
  }
}